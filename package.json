{
  "name": "dr-city",
  "version": "0.0.2",
  "author": "dr-js",
  "license": "MIT",
  "description": "Server of Dr.city",
  "keywords": [
    "Dr",
    "Dr-js",
    "JavaScript"
  ],
  "homepage": "https://github.com/dr-js/dr-city#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dr-js/dr-city.git"
  },
  "bugs": {
    "url": "https://github.com/dr-js/dr-city/issues"
  },
  "main": "library/index.js",
  "scripts": {
    "perf-webpack-stats": "cross-env NODE_ENV=production webpack --config config/library.conf.js --json > stats.json",
    "perf-webpack-stats-pack": "cross-env NODE_ENV=production webpack --config pack/config/library.conf.js --json > pack-stats.json",
    "example": "cross-env NODE_ENV=development webpack --watch --config config/example.conf.js",
    "example-pack": "cross-env NODE_ENV=development webpack --watch --config pack/config/example.conf.js",
    "example-pack-dll": "cross-env NODE_ENV=development webpack --watch --config pack/config/example-dll.conf.js",
    "library": "cross-env NODE_ENV=production webpack --config config/library.conf.js --progress=true",
    "library-pack": "cross-env NODE_ENV=production webpack --config pack/config/library.conf.js --progress=true",
    "library-pack-dll": "cross-env NODE_ENV=production webpack --config pack/config/library-dll.conf.js --progress=true",
    "build": "npm run build-clear && npm run library-pack-dll && npm run library-pack && npm run library",
    "build-clear": "dr-js --mode file-modify-delete --argument library",
    "publish-dev": "npm publish --tag dev"
  },
  "engines": {
    "node": ">=8.8.0",
    "npm": ">=5.5.0"
  },
  "devDependencies": {
    "dev-dep-web-react": "^0.0.7",
    "dr-js": "^0.6.0-dev.4",
    "firebase": "^4.6.2",
    "firebase-admin": "^5.5.0",
    "material-ui": "^1.0.0-beta.21",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  }
}
