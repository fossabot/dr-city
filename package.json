{
  "name": "dr-city",
  "version": "0.0.1",
  "author": "dr-js",
  "license": "MIT",
  "description": "Server of Dr.city",
  "keywords": [
    "Dr",
    "Dr-js",
    "JavaScript"
  ],
  "homepage": "https://github.com/dr-js/dr-city#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dr-js/dr-city.git"
  },
  "bugs": {
    "url": "https://github.com/dr-js/dr-city/issues"
  },
  "bin": "bin/index.js",
  "main": "library/index.js",
  "scripts": {
    "perf-webpack-stats": "cross-env NODE_ENV=production webpack --config config/library.conf.js --json > stats.json",
    "perf-webpack-stats-pack": "cross-env NODE_ENV=production webpack --config pack/config/library.conf.js --json > pack-stats.json",
    "example": "cross-env NODE_ENV=development webpack --watch --config config/example.conf.js",
    "library": "cross-env NODE_ENV=production webpack --config config/library.conf.js",
    "pack-example": "cross-env NODE_ENV=development webpack --watch --config pack/config/example.conf.js",
    "pack-library": "cross-env NODE_ENV=production webpack --config pack/config/library.conf.js",
    "build": "npm run library",
    "prepare": "npm run build"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "devDependencies": {
    "dev-dep-web-react": "^0.0.3",
    "dr-js": "^0.2.4",
    "firebase": "^4.3.1",
    "firebase-admin": "^5.2.1",
    "material-ui": "next",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
